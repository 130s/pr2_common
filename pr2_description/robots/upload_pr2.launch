    <param name="robot_meshes/gripper_v0/finger_tip_r.stl" binfile="$(find pr2_description)/meshes/gripper_v0/finger_tip_r.stl" />
    <param name="robot_meshes/gripper_v0/finger_tip_pad2_r.stl" binfile="$(find pr2_description)/meshes/gripper_v0/finger_tip_pad2_r.stl" />
    <param name="robot_meshes/base_v0/base_L.stl" binfile="$(find pr2_description)/meshes/base_v0/base_L.stl" />
